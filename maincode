Servo flANKLE; 
Servo frANKLE; 

void setup()
{    
  Serial.begin(9600); // start serial monitor
TODO: FIX THIS SHit
  blHIP.attach(13);
  brHIP.attach(12);  
  flHIP.attach(11);
  frHIP.attach(10);
