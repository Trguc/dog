  int brhHOME = 78;
  int flhHOME = 80;
  int frhHOME = 95;

  blKNEE.attach(9);
  brKNEE.attach(8);  
  flKNEE.attach(7);
  frKNEE.attach(6);
  int blkHOME = 100;
  int brkHOME = 60;
  int flkHOME = 95;
  int frkHOME = 75;


  blANKLE.attach(5);
  brANKLE.attach(4);  
  flANKLE.attach(3);
  frANKLE.attach(2);
  int blaHOME = 0;
  int braHOME = 0;
  int flaHOME = 0;
  int fraHOME = 140;

  blHIP.write(blhHOME);
  brHIP.write(brhHOME);
  flHIP.write(flhHOME);
  frHIP.write(frhHOME);

  blKNEE.write(blkHOME);
  brKNEE.write(brkHOME);
  flKNEE.write(flkHOME);
  frKNEE.write(frkHOME);


  //blANKLE.write(blaHOME);
